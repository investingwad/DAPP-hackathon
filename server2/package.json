{
  "name": "DAPP_Service",
  "version": "1.0.0",
  "description": "backend for DAPP service",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015\"",
    "build": "babel src -s -D -d dist --presets es2015",
    "start": "node --trace-warnings dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "clean": "rm -rf dist node_modules",
    "deploy": "yarn clean && yarn && yarn build && now",
    "deploy-pro": "yarn clean && yarn && yarn build && now --target production"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "bitbucket",
    "url": "https://bitbucket.org/blockstartOne/nodejs-bridge"
  },
  "dependencies": {
    "@liquidapps/dapp-client": "^2.0.3107",
    "async": "^3.1.0",
    "async-retry": "^1.2.3",
    "aws-sdk": "^2.436.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "big-integer": "^1.6.47",
    "binaryen": "^71.0.0",
    "body-parser": "^1.13.3",
    "btoa": "^1.2.1",
    "circular-json": "^0.5.9",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "cron": "^1.7.1",
    "dotenv": "^2.0.0",
    "eos-auth": "^2.1.22",
    "eosjs": "^20.0.0",
    "express": "^4.13.3",
    "express-useragent": "^1.0.12",
    "express-winston": "^3.2.1",
    "fs": "0.0.1-security",
    "google-contacts": "^0.1.5",
    "isomorphic-fetch": "^2.2.1",
    "jade": "^1.11.0",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "moment": "^2.29.2",
    "mongoose": "5.4.11",
    "mongoose-auto-increment": "5.0.1",
    "mongoose-sequence": "5.0.1",
    "morgan": "^1.8.0",
    "mysql": "^2.8.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.3.0",
    "node-ses": "^2.2.0",
    "nodemailer": "^6.0.0",
    "now-env": "^3.1.0",
    "randomstring": "^1.1.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "resource-router-middleware": "^0.6.0",
    "ua-parser-js": "^0.7.19",
    "util": "^0.11.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
